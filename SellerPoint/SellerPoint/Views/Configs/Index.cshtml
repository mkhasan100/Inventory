@model IEnumerable<SellerPoint.Models.Config>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryCharge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReceiptName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChalanName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DefaultWarehouse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DealerPriceChange)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryCharge)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReceiptName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChalanName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DefaultWarehouse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DealerPriceChange)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
